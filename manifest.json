{
  "manifest_version": 3,
  "name": "React Content Script",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": ["https://example.com/*"],
      "js": ["content-script/src/main.tsx"],
      "media": []
    }
  ]
}
